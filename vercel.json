{
  "installCommand": "cd frontend && npm ci --legacy-peer-deps",
  "buildCommand": "cd frontend && npx ng build --configuration production",
  "outputDirectory": "frontend/dist/frontend/browser",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://proyecto2-sensores-production.up.railway.app/:path*"
    }
  ]
}